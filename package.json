{
  "name": "laboratorio2",
  "version": "1.0.0",
  "description": "Trabajo Práctico Integrador - Laboratorio 2",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node index.js",
    "start:dev": "nodemon index.js",
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "database:migrate": "npx prisma migrate dev",
    "database:seed": "npx prisma migrate reset --preview-feature --force",
    "postinstall": "prisma generate",
    "build": "prisma generate && <actual-build-command>"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "keywords": [
    "agenda",
    "turnos",
    "medicos",
    "practica",
    "integradora"
  ],
  "author": "Federico González",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.21.0",
    "express-validator": "^7.2.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.5.0",
    "method-override": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/luxon": "^3.4.2",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.9.0",
    "husky": "^8.0.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "prisma": "^5.20.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:FedericoDG/laboratorio2.git"
  },
  "homepage": "https://github.com/FedericoDG/laboratorio2"
